pipeline { 
    agent any  
    tools { 
        maven 'Maven 3.23' 
        jdk 'jdk8' 
    }
    stages { 
        stage('Init') { 
            steps { 
               echo 'This is a minimal pipeline.' 
            }
        }
        stage ('Build') {
      steps {
            sh 'mvn clean package'
          }
        }
    }
}
